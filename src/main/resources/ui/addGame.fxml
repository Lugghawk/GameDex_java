<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<FlowPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="160.0"
          prefWidth="290.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.gamedex.ui.controller.AddGameController">
    <children>
        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="94.0" prefWidth="207.0">
            <children>
                <VBox prefHeight="192.0" prefWidth="239.0">
                    <children>
                        <HBox prefHeight="100.0" prefWidth="355.0" VBox.vgrow="NEVER">
                            <children>
                                <Label prefHeight="22.0" prefWidth="97.0" text="Game Name: ">
                                    <padding>
                                        <Insets top="5.0"/>
                                    </padding>
                                </Label>
                                <TextField fx:id="gameName"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="107.0" prefWidth="209.0" spacing="10.0">
            <children>
                <Button cancelButton="true" mnemonicParsing="false" onAction="#close" text="Cancel"/>
                <Button mnemonicParsing="false" onAction="#addGame" text="Add"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</FlowPane>
